[Unit]
Description=Teku Beacon chain daemon (Holesky-mev)
After=network-online.target
Wants=network-online.target

[Service]
EnvironmentFile=/etc/ethereum/teku-beacon-holesky-mev.conf
Environment="JAVA_OPTS=-Xmx5g"
ExecStart=/usr/bin/teku $ARGS
Restart=on-failure
RestartSec=5
User=ethereum
SyslogIdentifier=teku-beacon-holesky-mev
LimitNOFILE=65536
LimitNPROC=65536

[Install]
WantedBy=multi-user.target