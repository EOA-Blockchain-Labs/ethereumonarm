[Unit]
Description=Teku Validator Client (Gnosis)
After=network-online.target
Wants=network-online.target

[Service]
User=ethereum
EnvironmentFile=/etc/ethereum/teku-validator-gnosis.conf
ExecStart=/usr/bin/teku vc $ARGS
Restart=on-failure
RestartSec=5
WorkingDirectory=/home/ethereum
LimitNOFILE=1000000
LimitNPROC=65536
SyslogIdentifier=teku-validator-gnosis

[Install]
WantedBy=multi-user.target
